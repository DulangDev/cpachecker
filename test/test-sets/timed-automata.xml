    <tasks name="FischerProtocol2">
      <include>../programs/timedautomata/fischer/fischer2.cta</include>
    </tasks>
  
    <tasks name="FischerProtocol4">
      <include>../programs/timedautomata/fischer/fischer2.cta</include>
    </tasks>
  </rundefinition>

</benchmark>
