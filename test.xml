  <rundefinition name="test-comp19_code-coverage">
     <propertyfile>coverage-branches.prp</propertyfile>
  </rundefinition>


  <tasks name="test-comp">
    <includesfile>/home/sruland/TigerIntegration/test-comp.set</includesfile>
    <option name="-32"/>
  </tasks>



</benchmark>

